<div class="container">

  <div class="row">

      <div class="col-lg-12">
        <h1 class="page-header">Upload your image:</h1>

          <div class="form-group">
              <form action="/photo" method="POST" enctype="multipart/form-data">
                <div class="input-group">
                  <div class="input-group-addon"><span class="glyphicon glyphicon-search"></span></div>

                  <input type="file" class="form-control" name="image">
                  <span class="input-group-btn"><button type="submit" class="btn btn-default">Upload image</button></span>

                </div>
              </form>
          </div>

      </div>

  </div>
</div>

<div class="container">

  <div class="row">

    <div class="col-lg-12">
      <h1 class="page-header">Search your images:</h1>
        <!-- <label for="search-bar-label">Search your photos by category:</label> -->

        <select class="multiple-searchbar" multiple="multiple" style="width: 90%">
          <% @photolabels.each do |photolabel| %>
          <option value="<%= photolabel.label %>"><%= photolabel.label %></option>
          <% end %>
        </select>

      <button class="btn btn-default" id="selected-labels">Enter</button>
    </div>

  </div>

</div>

<div class="container">

    <div class="row">

        <div class="col-lg-12">
            <h1 class="page-header">Gallery:</h1>
        </div>

        <div id="image-board"></div>

    </div>
</div>
